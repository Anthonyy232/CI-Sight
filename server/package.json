{
  "name": "ci-sight-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npm run clean && tsc -p . && npx prisma generate",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.12.2",
    "bullmq": "^5.58.6",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.16.2",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
